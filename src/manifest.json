{
	"manifest_version": 2,
	"name": "Trello Extras",
	"version": "1",
	"description": "Adds some much needed extra functionality to Trello",
	"author": "barryels@gmail.com",
	"content_scripts": [
		{
			"exclude_globs": [],
			"include_globs": [
				"*"
			],
			"css": [
				"index.css"
			],
			"js": [
				"jquery.min.js",
				"index.user.js"
			],
			"matches": [
				"http://trello.com/*",
				"https://trello.com/*"
			],
			"run_at": "document_end"
		}
	],
	"converted_from_user_script": true
}
