{
	"manifest_version": 2,
	"content_scripts": [
		{
			"exclude_globs": [],
			"include_globs": [
				"*"
			],
			"css": [
				"css/index.css"
			],
			"js": [
				"js/index.user.js"
			],
			"matches": [
				"http://trello.com/*",
				"https://trello.com/*"
			],
			"run_at": "document_end"
		}
	],
	"converted_from_user_script": true,
	"browser_action": {
		"default_title": "",
		"default_icon": "app-icon-128.png",
		"default_popup": "popup.html"
	},
	"name": "Trello Extras (Userscript)",
	"version": "1.6.1",
	"description": "Some extra functionality for Trello",
	"author": "Barry Els <barryels@gmail.com>"
}